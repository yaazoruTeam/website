{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.ts",
    "scripts": {
        "test": "jest",
        "build": "tsc && tsc-alias",
        "build-schema": "ts-node -r tsconfig-paths/register ./src/db/buildSchema",
        "dev": "ts-node-dev --respawn --transpile-only --prefer-ts-exts -r tsconfig-paths/register src/index.ts",
        "dev:nodemon": "npx nodemon --legacy-watch --exec \"ts-node -r tsconfig-paths/register -r dotenv/config\" src/index.ts",
        "start": "node dist/src/index.js",
        "db:migrate": "docker-compose exec -T backend npx ts-node -r tsconfig-paths/register -r reflect-metadata ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
        "db:generate": "docker-compose exec -T backend npx ts-node -r tsconfig-paths/register -r reflect-metadata ./node_modules/typeorm/cli.js migration:generate src/migrations/$npm_config_name -d src/data-source.ts",
        "db:revert": "docker-compose exec -T backend npx ts-node -r tsconfig-paths/register -r reflect-metadata ./node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
        "db:show": "docker-compose exec -T backend npx ts-node -r tsconfig-paths/register -r reflect-metadata ./node_modules/typeorm/cli.js migration:show -d src/data-source.ts"
    },
    "dependencies": {
        "@google-cloud/speech": "^7.2.0",
        "@types/xlsx": "^0.0.36",
        "axios": "^1.7.9",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.6.1",
        "express": "^4.21.1",
        "ipaddr.js": "^2.2.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "media-typer": "^1.1.0",
        "model": "file:../model",
        "multer": "^2.0.1",
        "nodemon": "^3.1.10",
        "pg": "^8.13.3",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.2.2",
        "safer-buffer": "^2.1.2",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.27",
        "winston": "^3.17.0",
        "xlsx": "^0.18.5"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/crypto-js": "^4.2.2",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/knex": "^0.16.1",
        "@types/multer": "^1.4.13",
        "@types/node": "^22.18.12",
        "@types/pg": "^8.11.11",
        "@types/sequelize": "^4.28.20",
        "@types/supertest": "^6.0.3",
        "jest": "^29.7.0",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.8.3"
    }
}
